{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://qup.dating"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://qup.dating"
      }
    },
    "production": {
      "android": { "autoIncrement": true },
      "ios": { "autoIncrement": true },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://qup.dating"
      }
    }
  },
  "submit": {
    "production": {}
  }
}